    "import numpy as np\n",
    "f = open(\"input\", \"r\")\n",
    "sum = 0\n",
    "list1 = []\n",
    "list2 = []\n",
    "for i in f:\n",
    "    a = i.split()\n",
    "    list1.append(int(a[0]))\n",
    "    list2.append(int(a[1]))\n",
    " \n",
    "list1.sort()\n",
    "list2.sort()\n",
    "\n",
    "np.sum(abs(np.array(list1) - np.array(list2)))"
   ]
